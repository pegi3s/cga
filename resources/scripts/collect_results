#!/bin/bash

set -o nounset
set -o errexit

rm -rf ${working_dir}/results
mkdir -p ${working_dir}/results

cat ${working_dir}/cga_working_dir/*/05_*blast > ${working_dir}/results/blast
cat ${working_dir}/cga_working_dir/*/05_*pep > ${working_dir}/results/pep
cat ${working_dir}/cga_working_dir/*/05_*nuc > ${working_dir}/results/nuc
cat ${working_dir}/cga_working_dir/*/04_*results > ${working_dir}/results/results
